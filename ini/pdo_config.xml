<?xml version="1.0" encoding="UTF-8"?>

<vxpdo>
	<datasource name="vxweb">
		<!-- currently pgsql or mysql -->
		<driver>mysql</driver>
		<user>{ $env(db_user) }</user>
		<password>{ $env(db_password) }</password>
		<dsn>{ $env(dsn) }</dsn>
		<!-- or host and database name -->
		<!--
			<host>host</host>
			<dbname>database_name</dbname>
		-->
	</datasource>
</vxpdo>
